{
    "name": "example/laravel-api",
    "type": "project",
    "description": "Laravel API Example for Serverless Container Framework",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
